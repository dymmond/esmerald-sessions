[flake8]
max-line-length = 120
max-complexity = 12
ignore = E501, E203, B008, W503, C408, B009, B023, C417, C901, PT006, PT007, PT004, PT012, SIM401
select = C,E,F,W,B,B9
exclude = __init__.py
type-checking-pydantic-enabled = true
type-checking-fastapi-enabled = true
classmethod-decorators =
    classmethod
    validator
